    "nextra": "^3.2.5",
    "nextra-theme-docs": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "~5.8.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.35",
    "gh-pages": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "rimraf": "^5.0.0",
    "typedoc": "^0.27.0"
