    "@nestia/sdk": "catalog:samchon",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.6",
    "@prisma/adapter-pg": "catalog:prisma",
    "@prisma/client": "catalog:prisma",
    "@prisma/client-runtime-utils": "catalog:prisma",
    "@samchon/openapi": "catalog:samchon",
    "bcryptjs": "^3.0.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "catalog:libs",
    "prisma": "catalog:prisma",
    "tgrid": "catalog:samchon",
    "tstl": "catalog:samchon",
    "typia": "catalog:samchon",
    "uuid": "^11.1.0"
  },
